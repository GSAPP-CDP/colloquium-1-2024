# Ignore environment variable files
.env

# Node.js modules
node_modules/

# Logs
*.log
logs/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Directory for build outputs
dist/
build/

# OS-specific files
.DS_Store
Thumbs.db

# IDE-specific files
.vscode/
.idea/

# Optional npm cache directory
.npm/

# Ignore coverage reports
coverage/

# Ignore configuration files containing sensitive data
config.js

# Ignore other sensitive files or folders
*.pem
*.key
